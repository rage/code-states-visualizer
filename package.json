{
  "name": "code-states-visualizer",
  "version": "0.1.0",
  "main": "dist/app.js",
  "files": [
    "dist"
  ],
  "author": "Antti Leinonen <antti.leinonen@helsinki.fi>",
  "license": "MIT OR Apache-2.0",
  "scripts": {
    "start": "env NODE_ENV=development yarn run webpack-dev-server --progress --colors",
    "webpack": "env NODE_ENV=development node ./node_modules/.bin/webpack",
    "build": "rm -rf dist && NODE_ENV=production node ./node_modules/.bin/webpack -p",
    "build-travis": "NODE_ENV=production node ./node_modules/.bin/webpack -p",
    "lint": "yarn run eslint --fix --ext .js --ext .jsx",
    "test": "yarn run nyc ava",
    "coverage": "BABEL_ENV=production babel --out-dir=dist index.jsx",
    "prepublish": "yarn build"
  },
  "dependencies": {
    "codemirror": "^5.54.0",
    "core-js": "^3.6.5",
    "focus-visible": "^5.1.0",
    "rc-slider": "^9.2.4",
    "rc-tooltip": "^4.0.3",
    "react-codemirror": "^1.0.0",
    "react-codemirror2": "^7.1.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-action-analytics": "0.0.3",
    "redux-create-reducer": "^2.0.0",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.5",
    "sha.js": "^2.4.11",
    "store": "^2.0.12"
  },
  "peerDependencies": {
    "react": ">= 16",
    "react-dom": ">= 16"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-flow-strip-types": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-flow": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "app-module-path": "^2.2.0",
    "autoprefixer": "^9.8.0",
    "ava": "^3.8.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "flow-bin": "^0.125.1",
    "flow-typed": "^2.6.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "nyc": "^15.0.1",
    "postcss-class-prefix": "^0.3.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass-loader": "^8.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "resolutions": {
    "flow-typed/semver": "7.1.3"
  },
  "ava": {
    "files": [
      "./src/**/__tests__/*.js"
    ],
    "require": [
      "babel-register",
      "./resolve-modules.js"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "@babel/preset-flow",
      "@babel/preset-react",
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-flow-strip-types",
      "@babel/plugin-transform-runtime"
    ],
    "ignore": [
      "*-test.js"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  }
}
